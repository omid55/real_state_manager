                    <ColumnDefinition Width="150*" />
                    <ColumnDefinition Width="187*" />
                </Grid.ColumnDefinitions>
                <Button Name="search_button" Content="جستجو شود" Grid.Column="2" Grid.Row="1" TabIndex="10" Click="search_button_Click" Grid.ColumnSpan="2" Margin="28.02,0,0.007,0">
